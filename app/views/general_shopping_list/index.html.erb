<div >
<h1 >Shopping List</h1>
<div >
  <h2>Amount of food Items to buy: num</h2>
  <h2>Total value of food needed: $num</h2>
</div>
<table>
  <thead>
    <tr>
      <th >Food</th>
      <th >Quantity</th>
      <th > Price</th>
    </tr>
  </thead>
  <tbody>
    <% @recipes.each do | recipe | %>
    <tr>
      <th >
        <%= recipe.name %>
      </th>
      <td> <%= recipe.name %> </td>
      <td> $ <%= recipe.name %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>